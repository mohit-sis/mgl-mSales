                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/newPlotNumber_TextView"
                android:layout_marginTop="5dip"
                android:textColor="#ff000000" >
            </TextView>

            <TextView
                android:id="@+id/wingNoText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/newPlotNo"
                android:layout_marginTop="5dip"
                android:text="Current Wing Number: "
                android:textColor="#ff000000" >
            </TextView>

            <TextView
                android:id="@+id/wingNo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/wingNoText"
                android:layout_marginTop="5dip"
                android:textColor="#ff000000" >
            </TextView>

            <Button
                android:id="@+id/wingNumberButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/wingNo"
                android:layout_alignParentRight="true"
                android:layout_marginTop="10dip"
                android:padding="15dp"
                android:text="Edit" >
            </Button>

            <TextView
                android:id="@+id/newWingNumber_TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/wingNo"
                android:layout_marginTop="5dip"

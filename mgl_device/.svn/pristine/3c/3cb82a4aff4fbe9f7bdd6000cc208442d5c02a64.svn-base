            <TextView
                android:id="@+id/newFlatNumber_TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/flatNo"
                android:layout_marginTop="5dip"

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/buildNameText"
                android:layout_marginTop="5dip"
                android:textColor="#ff000000" >
            </TextView>

            <Button
                android:id="@+id/buildNameButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/buildName"
                android:layout_alignParentRight="true"
                android:layout_marginTop="10dip"
                android:padding="15dp"
                android:text="Edit" >
            </Button>

            <TextView
                android:id="@+id/newBuildName_TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/buildName"
                android:layout_marginTop="5dip"
                android:text="New Building Name: "
                android:textColor="#ff000000"
                android:visibility="gone" >
            </TextView>

            <TextView
                android:id="@+id/newBuildName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/newBuildName_TextView"
                android:layout_marginTop="5dip"
                android:textColor="#ff000000" >
            </TextView>

            <!-- Contact No (Mobile) -->

            <TextView
                android:id="@+id/contactNoMobileText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/newBuildName"
                android:layout_marginTop="5dip"
                android:text="Current Contact Number (Mobile): "
                android:textColor="#ff000000" >
            </TextView>

            <TextView
                android:id="@+id/contactMobileNo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/contactNoMobileText"
                android:layout_marginTop="5dip"
                android:textColor="#ff000000" >
            </TextView>

            <Button
                android:id="@+id/contactNoMobileButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/contactMobileNo"
                android:layout_alignParentRight="true"
                android:layout_marginTop="10dip"

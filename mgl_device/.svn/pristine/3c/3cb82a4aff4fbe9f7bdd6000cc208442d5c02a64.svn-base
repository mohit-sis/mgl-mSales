            <TextView
                android:id="@+id/newPlotNumber_TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/plotNo"
                android:layout_marginTop="5dip"

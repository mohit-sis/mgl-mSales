                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/newContactNoMobile"
                android:layout_marginTop="5dip"
                android:text="Current Contact Number (Home): "
                android:textColor="#ff000000" >
            </TextView>

            <TextView
                android:id="@+id/contactHomeNo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/contactNoHomeText"
                android:layout_marginTop="5dip"
                android:textColor="#ff000000" >
            </TextView>

            <Button
                android:id="@+id/contactNoHomeButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/contactHomeNo"
                android:layout_alignParentRight="true"
                android:layout_marginTop="10dip"
                android:padding="15dp"
                android:text="Edit" >
            </Button>

            <TextView
                android:id="@+id/newContactNoHome_TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/contactHomeNo"
                android:layout_marginTop="5dip"
                android:text="New Contact Number (Home)"
                android:textColor="#ff000000"
                android:visibility="gone" >
            </TextView>

            <TextView
                android:id="@+id/newContactNoHome"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/newContactNoHome_TextView"
                android:layout_marginTop="5dip"
                android:textColor="#ff000000" >
            </TextView>

            <!-- Contact No (Office) -->

            <TextView
                android:id="@+id/contactNoOfficeText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/newContactNoHome"
                android:layout_marginTop="5dip"
                android:text="Current Contact Number (Office): "
                android:textColor="#ff000000" >
            </TextView>

            <TextView
                android:id="@+id/contactOfficeNo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/contactNoOfficeText"
                android:layout_marginTop="5dip"
                android:textColor="#ff000000" >
            </TextView>

            <Button
                android:id="@+id/contactNoOfficeButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/contactOfficeNo"
                android:layout_alignParentRight="true"
                android:layout_marginTop="10dip"
                android:padding="15dp"
                android:text="Edit" >
            </Button>

            <TextView
                android:id="@+id/newContactNoOffice_TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/contactOfficeNo"
                android:layout_marginTop="5dip"
                android:text="New Contact Number (Office) "
                android:textColor="#ff000000"
                android:visibility="gone" >
            </TextView>

            <TextView
                android:id="@+id/newContactNoOffice"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/newContactNoOffice_TextView"
                android:layout_marginTop="5dip"
                android:textColor="#ff000000" >
            </TextView>

            <!-- Remark -->

            <TextView
                android:id="@+id/custUpdate_TextViewRemark"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/newContactNoOffice"
                android:layout_marginTop="6dip"
                android:text="Remark:"
                android:textColor="#ff000000" >
            </TextView>

            <EditText
                android:id="@+id/custUpdate_EditTextRemark"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/custUpdate_TextViewRemark"
                android:layout_marginTop="5dip"
                android:textColor="#ff000000">
            </EditText>
            
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:layout_alignParentBottom="true"
                android:orientation="horizontal"
                android:padding="5dp"
                android:weightSum="2" >

                <Button
                    android:id="@+id/cancelButton"
                    android:layout_width="0dip"
                    android:layout_height="35dp"
                    android:layout_weight="1"
                    android:background="#333333"
                    android:text="Cancel"
                    android:textColor="@color/white" />

                <LinearLayout
                    android:layout_width="20dp"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal" />

                <Button
                    android:id="@+id/updateButton"
                    android:layout_width="0dip"
                    android:layout_height="35dp"
                    android:layout_weight="1"
                    android:background="#bc3751"
                    android:text="Update"
                    android:textColor="@color/white" />
            </LinearLayout>
            
        </RelativeLayout>
    </ScrollView>

</LinearLayout><!--

                    -->

                    <Spinner
                        android:id="@+id/sp_code"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dip" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_lastMeterRedingLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/ll_code"

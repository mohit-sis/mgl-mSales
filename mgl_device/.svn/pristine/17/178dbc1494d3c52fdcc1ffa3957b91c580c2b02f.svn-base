                <LinearLayout
                    android:id="@+id/ll_reEnter"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/ll_premise"
                    android:layout_marginTop="5dp"
                    android:orientation="vertical"
                    android:visibility="gone" >

                    <TextView
                        android:id="@+id/tv_reEnter"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Re-Enter Meter Reading :"
                        android:textStyle="bold" />

                    <EditText
                        android:id="@+id/ed_reEnter"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dip"
                        android:inputType="number"
                        android:maxLength="250" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_code"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/ll_reEnter"
                    android:layout_marginTop="5dip"
                    android:orientation="vertical"
                    android:visibility="gone" >

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" >

                        <TextView
                            android:id="@+id/tv_code"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:layout_alignParentLeft="true"
                            android:text="Code :"
                            android:textColor="#000000"
                            android:textStyle="bold" />

                        <LinearLayout
                            android:id="@+id/ll_hindi_text_code"
                            android:layout_width="60dp"
                            android:layout_height="40dp"
                            android:layout_alignParentRight="true"
                            android:gravity="bottom|right"
                            android:orientation="vertical" >

                            <ImageView
                                android:id="@+id/hindi_text_icon_code"
                                android:layout_width="15dp"
                                android:layout_height="15dp"
                                android:layout_marginRight="8dp"
                                android:background="@drawable/i" />
                        </LinearLayout>
                    </RelativeLayout>
                    <!--
                     <TextView
                    android:id="@+id/tv_code"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Code :"

                            android:orientation="vertical" >

                            <ImageView
                                android:id="@+id/hindi_text_icon"
                                android:layout_width="15dp"
                                android:layout_height="15dp"
                                android:layout_marginRight="8dp"
                                android:background="@drawable/i" />
                        </LinearLayout>
                    </RelativeLayout>
